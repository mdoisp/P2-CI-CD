  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "swagger-ui-express": "^4.6.3",
    "morgan": "^1.10.0",
    "dotenv": "^16.4.5",
    "axios": "^1.10.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "semantic-release": "^21.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "sqlite3": "^5.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
